<style>
    #mapCanvas {
        width: 500px;
        height: 500px;
    }
</style>
@section Scripts{
    <script src="https://maps.googleapis.com/maps/api/js?key=AIzaSyD5mpQp3cfcXbn8Cw5GdPfKvHZuSHgEN8g"></script>
    <script src="~/Scripts/contactController.js"></script>
    <script>
        // This example displays a marker at the center of Australia.
        // When the user clicks the marker, an info window opens.11.075739, 106.568122
        function initMap() {
            const uluru = { lat: 11.075739, lng: 106.568122 };
            const map = new google.maps.Map(document.getElementById("mapCanvas"), {
                zoom: 18,
                center: uluru,
            });
            const contentString = '@Html.Raw(Model.Content)';
            const infowindow = new google.maps.InfoWindow({
                content: contentString,
            });
            const marker = new google.maps.Marker({
                position: uluru,
                map,
                title: "Địa chỉ",
            });
            marker.addListener("click", () => {
                infowindow.open({
                    anchor: marker,
                    map,
                    shouldFocus: false,
                });
            });
        }
        google.maps.event.addDomListener(window, 'load', initMap)
    </script>
    <script>
        setTimeout(function () {
            $('#successSow').fadeOut('slow');
        }, 2000);
    </script>
}
